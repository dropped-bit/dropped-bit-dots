{
	// Workspaces
	"hyprland/workspaces" : {
		"disable-scroll": true,
		"all-outputs": true,
		"warp-on-scroll": false,
		"on-click": "activate",
		"active-only": false,
		"all-outputs": true,
		"format": "{}",
		"format-icons": {
				"urgent": "",
				"active": "",
				"default": ""
		},
		"persistent_workspaces": {
		     "*": 5
		}       
    },
        "battery": {
        "format": "{capacity}% {icon}",
        "format-icons": ["", "", "", "", ""]
    },
    "clock": {
        "format-alt": "{:%a, %d. %b  %H:%M}"
    },
    "backlight": {
        // "device": "acpi_video1",
	"format": "{percent}% {icon}",
	"format-icons": ["", "", "", "", "", "", "", "", ""]
    },
}
